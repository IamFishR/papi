{
  "name": "papi",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "debug": "node --inspect src/server.js",
    "debug:break": "node --inspect-brk src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:journal": "jest --testMatch=\"**/tests/integration/v1/journal.test.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "makemigration": "node ./node_modules/sequelize-auto-migrations/bin/makemigration.js",
    "migrate": "node ./node_modules/sequelize-auto-migrations/bin/runmigration.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "migration:create": "sequelize-cli migration:generate --name",
    "seeder:create": "sequelize-cli seed:generate --name"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "mysql2": "^3.14.1",
    "node-cron": "^4.1.1",
    "sequelize": "^6.37.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sequelize-auto-migrations": "github:flexxnn/sequelize-auto-migrations",
    "sequelize-cli": "^6.6.3",
    "sequelize-mock": "^0.10.2",
    "supertest": "^7.1.1"
  }
}
